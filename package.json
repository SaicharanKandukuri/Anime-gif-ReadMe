    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@playwright/test": "^1.44.1",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.4",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "svelte": "^4.2.17",
    "svelte-check": "^3.7.1",
    "typescript": "^5.4.5",
    "vite": "^4.5.3"
  },
  "type": "module",
  "dependencies": {
    "bun-types": "^1.1.9"
  }
}
